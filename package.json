{
  "name": "open-file-in-new-vscode-window",
  "displayName": "Open File In New Vscode Window",
  "description": "右键点击文件/文件夹，可以选择在新vscode窗口中打开",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:open-file-in-new-vscode-window.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-file-in-new-vscode-window.openFileInNewVscodeWindow",
        "title": "Open In New Vscode Window"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "open-file-in-new-vscode-window.openFileInNewVscodeWindow",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duanlvxin/vscode-plugin-open-in-new-vscode-windw"
  }
}